<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <Import Project="SockLynx.Platform.targets" />
    <Import Project="SockLynx.DSO.targets" />

    <ItemGroup>
        <ProjectReference Include="$(CsDir)ConsoleBuild\ConsoleBuild.csproj">
            <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
            <SkipGetTargetFrameworkProperties>true</SkipGetTargetFrameworkProperties>
        </ProjectReference>
    </ItemGroup>

    <ItemGroup>
        <Compile Include="$(SlnDir)UnityProject\Assets\SockLynx\Scripts\*.cs" />
    </ItemGroup>

</Project>
